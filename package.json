{
  "name": "govuk-ui",
  "private": false,
  "version": "0.0.140",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://govuk-ui.github.io/govuk-ui/",
  "scripts": {
    "dev": "storybook dev --docs -p 6006",
    "test": "jest",
    "gen-components": "node ./src/scripts/gen-components.js",
    "gen-stories": "node ./src/scripts/gen-stories.js",
    "build-ts": "tsc",
    "build-sass": "sass ./src/scss:.storybook",
    "build-lib": "rollup -c",
    "patch": "npm version patch",
    "push": "git push",
    "publish-npm": "npm publish",
    "build-storybook": "npm run build-sass && storybook build --docs -o ./docs",
    "deploy-storybook": "npm run build-storybook && touch ./docs/.nojekyll && gh-pages -d docs -t true",
    "smash": "npm-run-all test build-lib patch push publish-npm deploy-storybook"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@storybook/addon-actions": "7.1.1",
    "@storybook/addon-docs": "7.1.1",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-interactions": "7.1.1",
    "@storybook/addon-links": "7.1.1",
    "@storybook/addon-mdx-gfm": "^7.1.1",
    "@storybook/react": "7.1.1",
    "@storybook/react-vite": "7.1.1",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "babel-loader": "9.1.3",
    "gh-pages": "5.0.0",
    "govuk-frontend": "4.7.0",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-html-parser": "2.0.2",
    "rollup": "3.26.3",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "sass": "1.64.1",
    "storybook": "7.1.1",
    "ts-jest": "29.1.1",
    "typescript": "4.9.5",
    "vite": "4.4.7"
  },
  "peerDependencies": {
    "govuk-frontend": "^4.6.0",
    "react": "^18.2.0",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7"
  }
}
